<?xml version="1.0"?>
<configuration>
	<connectionStrings/>
	<system.web>
		<compilation debug="true" />
		<authentication mode="Forms">
			<forms loginUrl="UserLogin.aspx">
				<credentials passwordFormat="Clear">
					<user name="iuser" password="password"/>
				</credentials>
			</forms>
		</authentication>
		<customErrors mode="Off"/>
	</system.web>
  <location path="Default.aspx">
    <system.web>
      <authorization>
        <deny users="?" />
      </authorization>
    </system.web>
  </location>
</configuration>
